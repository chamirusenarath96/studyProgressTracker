@model List<Subject>

<h2>All Subjects</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Weight</th>
    </tr>
    @foreach (var subject in Model)
    {
        <tr>
            <td>@subject.Id</td>
            <td>@subject.Name</td>
            <td>@subject.Weight</td>
        </tr>
    }
</table>
